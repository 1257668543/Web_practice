.container{
    width: 100%;
    height: 100vh;
    background-color: #f2f2f2;
}
.header{
    height: 40rpx;
}
.myGroup{
    background-color: #ffffff;
    margin: 0 45rpx;
    border-radius: 20rpx;
}
.title{
    height: 115rpx;
    color: #000000;
    border-bottom: 1rpx solid #f2f2f2;
    display: flex;
    align-items: center;
}
.title image{
    margin-left: 20rpx;
    width: 45rpx;
    height: 45rpx;
}
.title text{
    margin-left: 20rpx;
    font-weight: bold;
    font-size: 35rpx;
}
.item{
    height: 80rpx;
    display: flex;
    justify-content: space-between; 
    line-height: 80rpx;
    border-bottom: 1rpx solid #f2f2f2;
}
.item text{
    margin-left: 20rpx;
}
.pic{
    display: flex;
    justify-content: center;
    align-items: center;
}
.item image{
    width: 45rpx;
    height: 45rpx;
    border-radius: 50%;
}